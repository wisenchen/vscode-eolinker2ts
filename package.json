{
  "name": "eolinker2ts",
  "displayName": "Eolinker2Ts",
  "description": "将Eolinker的api转换成typescript使用的类型模板",
  "author": "wisen",
  "publisher": "wisen",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "keywords": [
    "eolinker",
    "typescript",
    "ts",
    "json",
    "类型",
    "转换"
],
  "icon": "resource/eolinker_logo.png",
  "repository": {
    "url": "https://github.com/wisenchen/vscode-eolinker2ts"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:eolinker2ts.paste2ts"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Eolinker2Ts",
      "type": "object",
      "properties": {
        "Eolinker2Ts.closeTypeNameInput": {
          "type": "boolean",
          "default": false,
          "description": "是否关闭类型名称输入框"
        },
        "Eolinker2Ts.tableColOrder": {
          "type": "object",
          "default": {
            "name": 0,
            "type": 1,
            "isRequired": 2,
            "desc": 3
          },
          "description": "从eolinker上复制的表格列顺序\n name: 类型名称，type: ts类型，isRequired: 是否可选，desc: 描述"
        },
        "Eolinker2Ts.ignoreFileds": {
          "type": "array",
          "default": [],
          "description": "忽略的字段"
        },
        "Eolinker2Ts.isReadonly": {
          "type": "boolean",
          "default": false,
          "description": "是否添加只读属性"
        },
        "Eolinker2Ts.orderingRule": {
          "type": "number",
          "enum": [
            0,
            1,
            2
          ],
          "default": 0,
          "description": "字段的排序规则,  0: 不排序, 1：按是否可选向下 , 2：按首字母向下"
        }
      }
    },
    "commands": [
      {
        "command": "eolinker2ts.paste2ts",
        "title": "将复制的内容转成ts模板和粘贴",
        "category": "Eolinker2Ts"
      }
    ],
    "keybindings": [
      {
        "command": "eolinker2ts.paste2ts",
        "key": "alt+shift+v"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "ts-loader": "^8.0.11",
    "typeof-jsonc": "^1.1.10",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  }
}
